%fieldset
  %h4 Set of Service Level Objective (SLO)
  .form-group
    = f.label "Attributes"
    = f.text_area :more_attributes, value: f.object.more_attributes.to_json, rows: 10,  class: 'form-control monospace md'
  .form-group
    = f.check_box :_destroy
    = f.label :_destroy, t('.remove_slo')
