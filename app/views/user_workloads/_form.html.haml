= form_for @user_workload do |f|
  - if @user_workload.errors.any?
    #error_explanation
      %h2= "#{pluralize(@user_workload.errors.count, "error")} prohibited this user_workload from being saved:"
      %ul
        - @user_workload.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :num_simultaneous_users
    = f.number_field :num_simultaneous_users
  .field
    = f.label :ingredient_id
    = f.number_field :ingredient_id
  .actions
    = f.submit 'Save'
